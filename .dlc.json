{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    { "pattern" : "^./blog" ,
      "replacement": "https://github.com/apache/dolphinscheduler-website/tree/master/blog"
    }
  ],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 10,
  "fallbackRetryDelay": "1000s",
  "aliveStatusCodes": [
    200,
    401
  ]
}
