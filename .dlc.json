{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^http://192.168.xx.xx"
    },
    {
      "pattern": "^http://127.0.0.1"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "/img/metadata-erd",
      "replacement": "https://dolphinscheduler.apache.org/img/metadata-erd"
    },
    {
      "pattern": "/asset/dolphinscheduler-netutils.jar",
      "replacement": "https://dolphinscheduler.apache.org/asset/dolphinscheduler-netutils.jar"
    }
  ],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 10,
  "fallbackRetryDelay": "1000s",
  "aliveStatusCodes": [
    200,
    401
  ]
}
